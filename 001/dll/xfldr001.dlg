DLGTEMPLATE ID_XFD_WELCOME LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  "Welcome to XWorkplace!", ID_XFD_WELCOME, 12, 12, 293, 156, , 
            FCF_SYSMENU | FCF_TITLEBAR
    BEGIN
        CONTROL         ID_XFLDRBITMAP, IDDLG_UNUSED, 225, 111, 23, 16, 
                        WC_STATIC, SS_BITMAP | WS_GROUP | WS_VISIBLE
        LTEXT           "XWorkplace was developed to add a number of things "
                        "to the Workplace Shell that are frequently missed a"
                        "nd IBM hasn't cared to implement so far.", 
                        IDDLG_UNUSED, 5, 111, 210, 29, DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        DEFPUSHBUTTON   "~OK", DID_OK, 205, 6, 80, 12
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "Welcome to XWorkplace!", IDDLG_UNUSED, 5, 141, 140, 
                        11, DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "The install program has created an object for the X"
                        "Folder Online Reference on your Desktop, which expl"
                        "ains XWorkplace's features in detail.", 
                        IDDLG_UNUSED, 5, 38, 280, 23, DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "Among other things you now get freely configurable "
                        "context menus, folder hotkeys, folder status bars, "
                        "display of folder contents in context menus, and mu"
                        "ch more.", IDDLG_UNUSED, 5, 81, 275, 23, 
                        DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "In order to get to know XWorkplace, simply click wi"
                        "th mouse button 2 on your Desktop, and you will alr"
                        "eady see the new context menu items.", IDDLG_UNUSED, 
                        5, 62, 280, 17, DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           """I'll never forget the first time I started Window"
                        "s, but I'm trying.""", IDDLG_UNUSED, 5, 6, 195, 17, 
                        DT_WORDBREAK
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
    END
END

