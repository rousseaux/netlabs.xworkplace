        MENUITEM    "~Open URL in existing window",       IDM_OPENURL
        MENUITEM    "~Open URL in new window",       IDM_OPENURLNEW
        MENUITEM    "~Close DDE",      IDM_CLOSE
        MENUITEM "", 1, MIS_SEPARATOR
        MENUITEM    "~Exit",       IDM_EXIT
    END
END

